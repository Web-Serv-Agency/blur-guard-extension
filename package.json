{
  "name": "blur-guard-extension",
  "displayName": "Blur guard extension",
  "version": "0.0.2",
  "description": "BlurGuard is a privacy-focused extension that blurs sensitive messages on social media until you're ready to reveal them. Customize your protection and keep your conversations private with ease, no matter where you are.",
  "author": "Web Serv",
  "license": "MIT",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/redux-persist": "^6.1.0",
    "@plasmohq/storage": "^1.12.0",
    "@reduxjs/toolkit": "^2.2.7",
    "plasmo": "0.89.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^9.1.2",
    "redux-persist-webextension-storage": "^1.0.2",
    "tailwindcss": "3.4.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@types/redux-persist-webextension-storage": "^1.0.3",
    "postcss": "8.4.33",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*.messenger.com/*"
    ]
  }
